program untung

kamus 
    b1, b2, untung : real

algoritma
    input(b1, b2)
    if b1 < b2 then
        untung = b2 - b1
        output("Peningkatan sebesar", untung)
    else if b1 > b2 then
        untung = b1 - b2
        output("Penurunan sebesar", untung)
    else
        output("Tetap")
    endif

endprogram